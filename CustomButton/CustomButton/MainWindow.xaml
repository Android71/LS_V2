<Window x:Class="CustomButton.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CustomButton"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.Background>
        <SolidColorBrush Color="Black"/>
    </Window.Background>
    <Grid Name="LayoutRoot">
        <StackPanel>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Center" Height="35" Stroke="Black" VerticalAlignment="Top" Width="75" RadiusY="20" RadiusX="20"/>
            <Button Width="75" Height="35"/>
        </StackPanel>
    </Grid>
</Window>
